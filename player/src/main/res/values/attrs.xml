<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="player_primary_color" format="color" />
    <declare-styleable name="IPlayer">
        <attr name="display" format="enum">
            <enum name="origin" value="0" />
            <enum name="normal" value="1" />
            <enum name="centerCrop" value="2" />
            <enum name="fitXY" value="3" />

        </attr>
        <attr name="enableEnlarge" format="boolean" />
        <attr name="enableWindow" format="boolean" />
        <attr name="mode" format="enum">
            <enum name="normal" value="0" />
            <enum name="full" value="1" />
        </attr>
    </declare-styleable>
</resources>